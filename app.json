{
  "expo": {
    "name": "travel-inn-app",
    "slug": "travel-inn-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sigh.png",
    "splash": {
      "image": "./assets/sigh.png",
      "resizeMode": "contain",
      "backgroundColor": "#1b1a1a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sigh.png",
        "backgroundColor": "#1b1a1a"
      },
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "@react-native-google-signin/google-signin"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
